# Defalut Initialization
. --autoload --no-alias ${HOME}/.config/yash/initialization/common

# Environment
. ${HOME}/.config/yash/export.sh

# Prompt
if [ "${LANG}" != 'C' ]; then
    . ${HOME}/.config/yash/prompt.sh
fi

# Alias
. ${HOME}/.config/yash/alias.sh

# Function
. ${HOME}/.config/yash/function.sh

# Screen
sleep 0.5
clean
